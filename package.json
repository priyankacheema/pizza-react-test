    "verbose": true,
    "bail": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "modulePaths": [
      "/shared/vendor/modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/src/index.js"
    ]
  },
  "version": "1.0.0",
  "description": "",
