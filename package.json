    "build:prod": "webpack --mode=production",
    "start": "webpack-dev-server --open --mode=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/priyankacheema/pizza-react-test.git"
  },
  "keywords": [],
